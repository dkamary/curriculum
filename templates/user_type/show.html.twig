{% extends 'base.html.twig' %}

{% block title %}UserType{% endblock %}

{% block body %}
    <h1>UserType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ user_type.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ user_type.description }}</td>
            </tr>
            <tr>
                <th>DeletedAt</th>
                <td>{{ user_type.deletedAt ? user_type.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ user_type.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_type_index') }}">back to list</a>

    <a href="{{ path('user_type_edit', {'id': user_type.id}) }}">edit</a>

    {{ include('user_type/_delete_form.html.twig') }}
{% endblock %}
