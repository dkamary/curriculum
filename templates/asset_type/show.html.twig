{% extends 'base.html.twig' %}

{% block title %}AssetType{% endblock %}

{% block body %}
    <h1>AssetType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ asset_type.id }}</td>
            </tr>
            <tr>
                <th>Mime</th>
                <td>{{ asset_type.mime }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ asset_type.name }}</td>
            </tr>
            <tr>
                <th>DeletedAt</th>
                <td>{{ asset_type.deletedAt ? asset_type.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ asset_type.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('asset_type_index') }}">back to list</a>

    <a href="{{ path('asset_type_edit', {'id': asset_type.id}) }}">edit</a>

    {{ include('asset_type/_delete_form.html.twig') }}
{% endblock %}
